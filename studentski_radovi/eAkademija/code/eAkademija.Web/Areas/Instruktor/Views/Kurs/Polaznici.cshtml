@model eAkademija.Web.Areas.Instruktor.ViewModels.KursPregledVM

@{
    ViewBag.Title = "Polaznici";
}

<div class="row page-content">

    <div class="col-md-12">

        <ol class="breadcrumb">
            <li><a href="~/">Home</a></li>
            <li><a href="~/Instruktor/Kurs">Kursevi</a></li>
            <li class="active">@Model.KursNaziv</li>
        </ol>

        <div class="page-header">
            <h1>Polaznici <small>@Model.KursNaziv</small></h1>
        </div>

        <div class="row">
            <div id="PolazniciWrapper" class="col-sm-10 col-sm-offset-1" style="padding: 15px">
                @Html.Action("Index", "Polaznik", new { @kursId = Model.KursId })
            </div>
        </div>

    </div>
</div>
